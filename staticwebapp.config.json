{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/images/*.{png,jpg,webp,svg}", "/assets/css/*.css", "/assets/js/*.js"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html",
      "statusCode": 404
    }
  },
  "globalHeaders": {
    "Strict-Transport-Security": "max-age=31536000; includeSubDomains"
  },
  "trailingSlash": "never"
}